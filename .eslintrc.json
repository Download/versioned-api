{
    "plugins": ["node"],
    "extends": "standard",
    "globals": {
      "test": true,
      "expect": true,
      "describe": true
    },
    "rules": {
      "no-throw-literal": 0,
      "no-useless-escape": 0,
      "no-unused-vars": ["error", {"varsIgnorePattern": "^_", "argsIgnorePattern": ".*"}]
    }
}
